<Dictionary xmlns="http://www.opengis.net/gml/3.2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:diggs="http://diggsml.org/schemas/2.6" xmlns:gml="http://www.opengis.net/gml/3.2"
    xsi:schemaLocation="http://diggsml.org/schemas/2.6 https://raw.githubusercontent.com/DIGGSml/diggs-schema/2.6/Diggs.xsd"
    gml:id="{{DictionaryName.DictionaryID}}">
    <description>
        {{DictionaryName.Description}}
    </description>
    <identifier codeSpace="http://diggsml.org/dictionaries/{{DictionaryName.DictionaryName}}.xml">{{DictionaryName.DictionaryID}}</identifier>
    <KYNLoop name="Definitions" loopby="table" loopon="Definitions">
        <dictionaryEntry>
            <diggs:Definition gml:id="{{Definitions.Code}}">
                <description>{{Definitions.Description}}</description>
                <identifier codeSpace="http://diggsml.org/dictionaries/{{DictionaryName.DictionaryName}}.xml">{{Definitions.Code}}</identifier>
                <name>{{Definitions.Name}}</name>
                <KYNCondition command="ISDATAPRESENT" sheet="Definitions" Column="DataType">
                <diggs:dataType>{{Definitions.DataType}}</diggs:dataType>
                </KYNCondition>
                <KYNCondition command="ISDATAPRESENT" sheet="Definitions" Column="UomType">
                <diggs:uomType>{{Definitions.UOMType}}</diggs:uomType>
                </KYNCondition>
                <KYNCondition command="ISDATAPRESENT" sheet="Definitions" Column="Authority">
                <diggs:authority>{{Definitions.Authority}}</diggs:authority>
                </KYNCondition>
                <KYNLoop name="Associated Elements" loopon="AssociatedElements" filterfields="Code" filtervalues="{{Definitions.Code}}">
                <diggs:associatedElement>{{AssociatedElements.Element}}</diggs:associatedElement>
                </KYNLoop>
            </diggs:Definition>
        </dictionaryEntry>
    </KYNLoop>
</Dictionary>
